root = true

[*.java]
charset = utf-8
indent_size = 4
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true
max_line_length = 140
ij_continuation_indent_size = 8
ij_java_imports_layout = javax.**, java.**, |, *, |, $*

[*.{yml,xml}]
charset = utf-8
indent_size = 2
indent_style = space
insert_final_newline = true
trim_trailing_whitespace = true
